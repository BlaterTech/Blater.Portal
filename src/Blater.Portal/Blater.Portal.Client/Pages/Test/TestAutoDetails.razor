@page "/Test/TestAutoDetails"
@using Blater.Frontend.Client.Auto.AutoBuilders.Types.Details
@using Blater.Portal.Client.Models

<div class="pa-2">
    <AutoDetailsBuilder
        T="Employee"
        Model="@Employee"
        Id="@Employee.Id"/>
</div>

@code {

    private Employee Employee { get; init; } = new()
    {
        Id = Ulid.NewUlid(),
        CreatedAt = DateTimeOffset.UtcNow,
        UpdatedAt = DateTimeOffset.UtcNow,
        Name = "Employee 1",
        Position = "CPA",
        Rating = 5,
        Salary = 75_000,
        YearsEmployed = 5
    };
}